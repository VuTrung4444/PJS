				end
			end
		})

		DailySection:AddToggle({ 
			Name = "Auto Spin Clan",
			Flag = "AutoSpinClan",
			Callback = function(state)
				-- Ki·ªÉm tra n·∫øu √≠t nh·∫•t m·ªôt trong ba toggle sƒÉn clan ƒë∆∞·ª£c b·∫≠t
				if state and (huntLegendary or huntMythic or huntSupreme) then
					ClanSpinEnabled = true
					autoSpinClan()
				else
