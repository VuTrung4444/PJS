	local activeToggle = nil

	for _, weapon in ipairs(weapons) do
		local toggle = KillAura:AddToggle({
			Name = weapon,
			Flag = "KillAuraWeapon_" .. weapon,
			Callback = function(state)
				if state then
					-- T·∫Øt toggle tr∆∞·ªõc ƒë√≥ n·∫øu c√≥ v√† kh√°c v·ªõi toggle hi·ªán t·∫°i
					if activeToggle and activeToggle ~= toggle then
						activeToggle:SetState(false)
					end
					-- C·∫≠p nh·∫≠t toggle hi·ªán t·∫°i v√† thi·∫øt l·∫≠p Method
					activeToggle = toggle
					Method = weapon
					print("Weapon Selected: " .. weapon)
				else
					-- Ch·ªâ clear n·∫øu ch√≠nh toggle hi·ªán t·∫°i b·ªã t·∫Øt
					if activeToggle == toggle then
						activeToggle = nil
						Method = nil
						print("Weapon Deselected")
					end
				end
			end
		})

		toggles[weapon] = toggle
	end

