	--MoveAround--

	local MoveLoop = nil
	local MoveLoopRunning = false
	local isTweening = false
	local CurrentTween = nil
	local LastVisitedBoss = nil
	local VisitedBosses = {}
	local AllBosses = {}
	local LastCharacter = nil

	local function startMoveLoop()
		if MoveLoopRunning then return end
		MoveLoopRunning = true

		MoveLoop = task.spawn(function()
			local player = game.Players.LocalPlayer
			local character = player.Character or player.CharacterAdded:Wait()
			LastCharacter = character

			while getgenv().MoveAround do
				if not character or not character:FindFirstChild("HumanoidRootPart") then
					character = player.CharacterAdded:Wait()
					task.wait(1)
				end

				UpdateActiveBossLocations()
				if #ActiveBossLocations == 0 then
					continue
				end

				if isTweening then
					print("[MoveLoop] ƒêang tween, ƒë·ª£i xong r·ªìi m·ªõi ƒëi ti·∫øp.")
					task.wait(1)
					continue
				end

				if #AllBosses == 0 then
					for _, boss in pairs(ActiveBossLocations) do
						table.insert(AllBosses, boss)
					end
				end

				local candidateBosses = {}
				for _, boss in pairs(ActiveBossLocations) do
					if not VisitedBosses[boss.Name] then
						table.insert(candidateBosses, boss)
					end
				end

				if #candidateBosses == 0 then
					VisitedBosses = {}
					candidateBosses = ActiveBossLocations
				end

				table.sort(candidateBosses, function(a, b)
					return GetDistance(a.Position) < GetDistance(b.Position)
				end)

				local boss = candidateBosses[1]
				if not boss then
					task.wait(1)
					continue
				end

				isTweening = true

				if CurrentTween then
					CurrentTween:Cancel()
					CurrentTween = nil
				end

				local bossCFrame = CFrame.new(boss.Position)
				CurrentTween = Tween(bossCFrame)

				repeat
					task.wait(0.1)
				until GetDistance(boss.Position) < 20 or not getgenv().MoveAround

				if CurrentTween then
					CurrentTween:Cancel()
					CurrentTween = nil
				end

				VisitedBosses[boss.Name] = true
				LastVisitedBoss = boss

				task.wait(1)
				isTweening = false
			end

			MoveLoopRunning = false
		end)
	end

	game:GetService("RunService").Heartbeat:Connect(function()
		local player = game.Players.LocalPlayer
		local character = player.Character

		if getgenv().MoveAround and not MoveLoopRunning then
			startMoveLoop()
		elseif not getgenv().MoveAround and MoveLoop then
			task.cancel(MoveLoop)
			MoveLoop = nil
			MoveLoopRunning = false
		elseif getgenv().MoveAround and character ~= LastCharacter then
			LastCharacter = character
			task.wait(4)
			if MoveLoop then
				task.cancel(MoveLoop)
				MoveLoop = nil
				MoveLoopRunning = false
			end
			task.wait(2)
			if getgenv().MoveAround then
				startMoveLoop()
			end
		end
	end)

	function StopMoveAround()
		getgenv().MoveAround = false

		-- H·ªßy v√≤ng l·∫∑p n·∫øu ƒëang ch·∫°y
		if MoveLoop then
			task.cancel(MoveLoop)
			MoveLoop = nil
		end

		-- H·ªßy tween n·∫øu ƒëang tween
		if CurrentTween then
			CurrentTween:Cancel()
			CurrentTween = nil
		end

		-- Reset l·∫°i to√†n b·ªô bi·∫øn
		MoveLoopRunning = false
		isTweening = false
		LastVisitedBoss = nil
		VisitedBosses = {}
		AllBosses = {}
	end


	--// Update Labels / Dropdowns \\--
	spawn(function()
		while task.wait(4) do
			updateLabels()
		end
	end)

	local VirtualUser = game:GetService("VirtualUser")
	local Players = game:GetService("Players")

	-- H√†m t·∫°o th·ªùi gian random ki·ªÉu t·ª± nhi√™n, c√≥ l√∫c nhanh l√∫c ch·∫≠m
	local function naturalWaitTime()
		local baseTime = 600 -- 10 ph√∫t
		local offset = math.random(-120, 180) -- l·ªách ng·∫´u nhi√™n t·ª´ -2 ph√∫t ƒë·∫øn +3 ph√∫t
		return math.max(300, baseTime + offset) -- ƒë·∫£m b·∫£o t·ªëi thi·ªÉu 5 ph√∫t
	end

	spawn(function()
		while true do
			wait(naturalWaitTime()) 
			if Players.LocalPlayer and Players.LocalPlayer.Character then
				VirtualUser:CaptureController()
				VirtualUser:ClickButton2(Vector2.new(math.random(100, 1820), math.random(100, 980))) 
			end
		end
	end)

	--[[MiscSection:AddToggle({
		Name = "Fake Name/DisplayName",
		Default = false,
		Callback = function(state)
